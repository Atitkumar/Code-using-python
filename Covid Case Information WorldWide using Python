# Get current COVID cases(WorldWide)
#import covid

from covid import Covid

covid = Covid()

def covid_worldWideInfo():
    print("Active- ",covid.get_total_active_cases())
    print("Confirmed- ",covid.get_total_confirmed_cases())
    print("Recoverd- ",covid.get_total_recovered())
    print("Death- ",covid.get_total_deaths())

covid_worldWideInfo()
